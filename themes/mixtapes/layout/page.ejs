<main class="page-main">
  <article class="post">
    <header class="post-header">
      <h1 class="post-title"><%= page.title %></h1>
    </header>
    <section class="post-content">
      <%- page.content %>
    </section>
  </article>
</main>
<section class="page-index">
  <% var posts = get_posts({
    orderby: 'date',
    order: -1,
    count: 0
  }); %>
  <% posts.each(function(post){ %><%- partial('_partials/postPreview', {post: post}) %><% }) %>
</section>
